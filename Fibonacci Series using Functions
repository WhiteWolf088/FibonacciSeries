def fib(n):           
    a=0
    b=1
    if n>=0:
        if n==1:
            print(a)
        else:
            print(a)
            print(b)
            for i in range(2,n):
                c=a+b
                a=b
                b=c
                print(c)
    else:
        print("Invalid Number")
fib(int(input("Enter the number of element do you want in your FibonacciSeries  : ")))
